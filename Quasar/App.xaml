        <!--#region Styles-->
        <Style x:Key="CutieLabelStyle" TargetType="{x:Type Label}">
            <Setter Property="FontFamily" Value="{StaticResource PoppinsRegular}"/>
            <Setter Property="Foreground" Value="{StaticResource CutieTextColor}"/>
        </Style>
        <Style x:Key="CutieTextBlockStyle" TargetType="{x:Type TextBlock}">
            <Setter Property="FontFamily" Value="{StaticResource PoppinsRegular}"/>
            <Setter Property="Foreground" Value="{StaticResource CutieTextColor}"/>
            <Setter Property="TextWrapping" Value="NoWrap" />
        </Style>
        <Style x:Key="CutieTextBoxStyle" TargetType="{x:Type TextBox}">
            <Setter Property="FontFamily" Value="{StaticResource PoppinsRegular}"/>
            <Setter Property="Foreground" Value="Black"/>
            <Setter Property="TextWrapping" Value="NoWrap" />
            <Setter Property="Template" Value="{StaticResource CutieTextBoxTemplate}" />
        </Style>
        <!--#endregion-->
        
        <!--#region Action Buttons-->
        <SolidColorBrush x:Key="ActionButtonColor" Color="#808080"/>
        
        <!--#region Styles-->
        <Style x:Key="ActionTrashPathStyle" TargetType="{x:Type Path}">
            <Setter Property="Data" Value="M6,19a2.006,2.006,0,0,0,2,2h8a2.006,2.006,0,0,0,2-2V9a2.006,2.006,0,0,0-2-2H8A2.006,2.006,0,0,0,6,9ZM18,4H15.5l-.71-.71a1.009,1.009,0,0,0-.7-.29H9.91a1.009,1.009,0,0,0-.7.29L8.5,4H6A1,1,0,0,0,6,6H18a1,1,0,0,0,0-2Z" />
            <Setter Property="Fill" Value="{StaticResource ActionButtonColor}" />
            <Setter Property="Stroke" Value="{StaticResource ActionButtonColor}" />
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Fill" Value="#cf6679"/>
                    <Setter Property="Stroke" Value="#cf6679"/>
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="ActionFolderPathStyle" TargetType="{x:Type Path}">
            <Setter Property="Data" Value="M22 16V4c0-1.1-.9-2-2-2H8c-1.1 0-2 .9-2 2v12c0 1.1.9 2 2 2h12c1.1 0 2-.9 2-2zm-10.6-3.47l1.63 2.18 2.58-3.22c.2-.25.58-.25.78 0l2.96 3.7c.26.33.03.81-.39.81H9c-.41 0-.65-.47-.4-.8l2-2.67c.2-.26.6-.26.8 0zM2 7v13c0 1.1.9 2 2 2h13c.55 0 1-.45 1-1s-.45-1-1-1H5c-.55 0-1-.45-1-1V7c0-.55-.45-1-1-1s-1 .45-1 1z" />
            <Setter Property="Fill" Value="{StaticResource ActionButtonColor}" />
            <Setter Property="Stroke" Value="{StaticResource ActionButtonColor}" />
            <Style.Triggers>
