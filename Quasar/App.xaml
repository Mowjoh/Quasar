<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Quasar"
             xmlns:System="clr-namespace:System;assembly=mscorlib" xmlns:Controls="clr-namespace:Quasar.Controls" xmlns:Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero2" x:Class="Quasar.App"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BooleanToVisibilityConverter x:Key="BoolVisi" />

        <!--Valley of the Old Carcasses meant to rot in hell -->
        <!--#region Old Colors-->
        <SolidColorBrush x:Key="QD0" Color="#121212"/>
        <SolidColorBrush x:Key="QD1" Color="#1e1e1e"/>
        <SolidColorBrush x:Key="QD2" Color="#2e2e2e"/>
        <SolidColorBrush x:Key="QD3" Color="#3e3e3e"/>

        <SolidColorBrush x:Key="QuasarButtonBackground" Color="White"/>
        <SolidColorBrush x:Key="QuasarButtonBackgroundPressed" Color="#CCCCCC"/>
        <SolidColorBrush x:Key="QuasarButtonText" Color="PaleVioletRed"/>

        <SolidColorBrush x:Key="QuasarBackgroundDark" Color="#262833"/>
        <SolidColorBrush x:Key="QuasarBackgroundLight" Color="#262833"/>
        <SolidColorBrush x:Key="QuasarBackgroundAccent" Color="#262833"/>
        <SolidColorBrush x:Key="QuasarTextColorAccent" Color="#4acfac"/>
        <SolidColorBrush x:Key="QuasarTextColor" Color="#ececec"/>
        <!--#endregion-->

        <!--#region Old Styles-->
        <Style TargetType="{x:Type ListBox}" x:Key="ModListStyle">
            <Setter Property="ItemsPanel">
                <Setter.Value>
                    <ItemsPanelTemplate>
                        <WrapPanel Width="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ActualWidth}"/>
                    </ItemsPanelTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="QuasarListBox" TargetType="{x:Type ListBox}">
            <Setter Property="Background" Value="{DynamicResource QD1}"/>
        </Style>

        <Style x:Key="QuasarList" TargetType="ListBox" >
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ListBoxItem">
                        <Border BorderThickness="1" BorderBrush="Transparent" 
                                Margin="5,0,5,0" Background="Transparent" Name="itemBorder" Padding="5,0,0,0">
                            <ContentPresenter />
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsSelected" Value="True">
                                <Setter TargetName="itemBorder" Property="BorderBrush" Value="Yellow"></Setter>
                            </Trigger>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="itemBorder" Property="BorderBrush" Value="Pink"></Setter>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="ControlStyle" TargetType="{x:Type Controls:ModListItem}">
            <Setter Property="Height" Value="30"/>
            <Style.Triggers>
                <DataTrigger Binding="{Binding Smol}" Value="true">
                    <Setter Property="Height" Value="30"/>
                </DataTrigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="QuasarButton" TargetType="{x:Type Button}">
            <Setter Property="Background" Value="{StaticResource QuasarButtonBackground}"/>
            <Setter Property="Foreground" Value="{StaticResource QuasarButtonText}"/>
        </Style>

        <Style x:Key="QuasarLabel" TargetType="{x:Type Label}">
            <Setter Property="Foreground" Value="{StaticResource QuasarTextColor}"/>
            <Setter Property="FontFamily" Value="/Controls/fonts/sofiapro-light.otf"/>
        </Style>

        <Style x:Key="QuasarListView" TargetType="{x:Type ListView}">
            <Setter Property="BorderThickness" Value="0" />
        </Style>

        <Style x:Key="QuasarComboBox" TargetType="{x:Type ComboBox}">

        </Style>

        <Style x:Key="QuasarDatagrid" TargetType="{x:Type DataGrid}">
            <Setter Property="Background" Value="Black"/>
            <Setter Property="Foreground" Value="White"/>

        </Style>

        <Style x:Key="QuasarDataGridRow" TargetType="{x:Type DataGridRow}">
            <Setter Property="Background" Value="Black"/>
            <Setter Property="Foreground" Value="#C0C0C0"/>
            <Setter Property="BorderBrush" Value="Pink"/>
        </Style>
        <Style x:Key="QuasarDataGridCell" TargetType="{x:Type DataGridCell}">
            <Setter Property="BorderBrush" Value="#C0C0C0"/>
        </Style>
        <Style x:Key="QuasarDataGridHeader" TargetType="{x:Type DataGridColumnHeader}">
            <Setter Property="Background" Value="Black"/>
            <Setter Property="Foreground" Value="White"/>
        </Style>
        <!--#endregion-->

        <!--#region Old Templates-->
        <ControlTemplate TargetType="Button" x:Key="QuasarButtonTemplate">
            <Grid Height="Auto" Width="Auto">
                <Border Height="Auto" Width="Auto">
                    <Path Stroke="Black" StrokeThickness="1" Fill="{StaticResource QuasarButtonBackground}">
                        <Path.Data>
                            <RectangleGeometry Rect="0,0,250,50" RadiusX="30" RadiusY="30" />
                        </Path.Data>
                        <Path.Style>
                            <Style TargetType="Path">
                                <Setter Property="Fill" Value="{StaticResource QuasarButtonBackground}"/>
                            </Style>
                        </Path.Style>
                    </Path>
                </Border>
                <Label VerticalAlignment="Center" Content="{TemplateBinding Button.Content}" HorizontalAlignment="Center"  FontSize="18">
                    <Label.Style >
                        <Style TargetType="Label">
                            <Setter Property="Foreground" Value="{DynamicResource CutieSecondaryColor}"/>
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding OKButtonEnabled}" Value="False">
                                    <Setter Property="Foreground" Value="Gray"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Label.Style>
                </Label>

            </Grid>
        </ControlTemplate>
        <!--#endregion-->
        
        <!--Valley of the new -->
        <!--#region Fonts-->
        <FontFamily x:Key="PoppinsThin">pack://application:,,,/Fonts/#Poppins Thin</FontFamily>
        <FontFamily x:Key="PoppinsExtraLight">pack://application:,,,/Fonts/#Poppins Extra Light</FontFamily>
        <FontFamily x:Key="PoppinsLight">pack://application:,,,/Fonts/#Poppins Light</FontFamily>
        <FontFamily x:Key="PoppinsRegular">pack://application:,,,/Fonts/#Poppins Regular</FontFamily>
        <FontFamily x:Key="PoppinsMedium">pack://application:,,,/Fonts/#Poppins Medium</FontFamily>
        <FontFamily x:Key="PoppinsSemiBold">pack://application:,,,/Fonts/#Poppins Semi Bold</FontFamily>
        <FontFamily x:Key="PoppinsBold">pack://application:,,,/Fonts/#Poppins Bold</FontFamily>
        <FontFamily x:Key="PoppinsExtraBold">pack://application:,,,/Fonts/#Poppins ExtraBold</FontFamily>
        <FontFamily x:Key="PoppinsBlack">pack://application:,,,/Fonts/#Poppins Black</FontFamily>

        <FontFamily x:Key="PoppinsThinItalic">pack://application:,,,/Fonts/#Poppins Thin Italic</FontFamily>
        <FontFamily x:Key="PoppinsExtraLightItalic">pack://application:,,,/Fonts/#Poppins ExtraLight Italic</FontFamily>
        <FontFamily x:Key="PoppinsLightItalic">pack://application:,,,/Fonts/#Poppins Light Italic</FontFamily>
        <FontFamily x:Key="PoppinsItalic">pack://application:,,,/Fonts/#Poppins Italic</FontFamily>
        <FontFamily x:Key="PoppinsMediumItalic">pack://application:,,,/Fonts/#Poppins Medium Italic</FontFamily>
        <FontFamily x:Key="PoppinsSemiBoldItalic">pack://application:,,,/Fonts/#Poppins SemiBold Italic</FontFamily>
        <FontFamily x:Key="PoppinsBoldItalic">pack://application:,,,/Fonts/#Poppins Bold Italic</FontFamily>
        <FontFamily x:Key="PoppinsExtraBoldItalic">pack://application:,,,/Fonts/#Poppins ExtraBold Italic</FontFamily>
        <FontFamily x:Key="PoppinsBlackItalic">pack://application:,,,/Fonts/#Poppins Black Italic</FontFamily>
        <!--#endregion-->

        <!--#region Base Colors -->
        <SolidColorBrush x:Key="CutiePrimaryColor" Color="#147a85" />
        <SolidColorBrush x:Key="CutieSecondaryColor" Color="#748bf2" />
        <SolidColorBrush x:Key="CutieErrorColor" Color="#CF6679" />
        <SolidColorBrush x:Key="CutieValidationColor" Color="#66CF66" />
        <!--#endregion-->

        <!--#region Text Colors-->
        <SolidColorBrush x:Key="CutieTextColor" Color="#FFFFFF" />
        <SolidColorBrush x:Key="CutieTextMediumColor" Color="#FFFFFF" Opacity="99" />
        <SolidColorBrush x:Key="CutieTextDisabledColor" Color="#FFFFFF" Opacity="61" />
        <!--#endregion-->

        <!--#region Background Colors -->
        <SolidColorBrush x:Key="CutieBG00" Color="#121212" />
        <SolidColorBrush x:Key="CutieBG01" Color="#1E1E1E" />
        <SolidColorBrush x:Key="CutieBG02" Color="#222222" />
        <SolidColorBrush x:Key="CutieBG03" Color="#242424" />
        <SolidColorBrush x:Key="CutieBG04" Color="#272727" />
        <SolidColorBrush x:Key="CutieBG06" Color="#2C2C2C" />
        <SolidColorBrush x:Key="CutieBG08" Color="#2E2E2E" />
        <SolidColorBrush x:Key="CutieBG12" Color="#333333" />
        <SolidColorBrush x:Key="CutieBG16" Color="#353535" />
        <SolidColorBrush x:Key="CutieBG24" Color="#383838" />
        <!--#endregion-->
        
        <!--#region Mod Colors-->
        <SolidColorBrush x:Key="DefaultColor" Color="#8c8c94"/>
        <SolidColorBrush x:Key="SkinColor" Color="#bc3991"/>
        <SolidColorBrush x:Key="MapColor" Color="#d5c53d"/>
        <SolidColorBrush x:Key="SoundColor" Color="#FFFFFF"/>
        <SolidColorBrush x:Key="GamefileColor" Color="#c96e2d"/>
        <SolidColorBrush x:Key="GUIColor" Color="#53cec1"/>
        <!--#endregion-->

        <!--#region Templates -->
        <ControlTemplate x:Key="CutieButtonTemplate" TargetType="{x:Type Button}">
            <Grid MaxHeight="38" Height="38">
                <Border CornerRadius="8" Background="{StaticResource CutiePrimaryColor}" Height="38">
                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center">
                        <ContentPresenter.ContentTemplate>
                            <DataTemplate>
                                <TextBlock Text="{Binding}" FontSize="17" FontFamily="{StaticResource PoppinsMedium}" Foreground="{StaticResource CutieTextColor}" />
                            </DataTemplate>
                        </ContentPresenter.ContentTemplate>
                    </ContentPresenter>
                </Border>
            </Grid>
        </ControlTemplate>
        <ControlTemplate x:Key="CutieSmallButtonTemplate" TargetType="{x:Type Button}">
            <Grid MaxHeight="32" Height="32">
                <Border CornerRadius="10" Background="{StaticResource CutiePrimaryColor}" Height="32">
                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center">
                        <ContentPresenter.ContentTemplate>
                            <DataTemplate>
                                <TextBlock Text="{Binding}" FontSize="15" FontFamily="{StaticResource PoppinsMedium}" Foreground="{StaticResource CutieTextColor}" />
                            </DataTemplate>
                        </ContentPresenter.ContentTemplate>
                    </ContentPresenter>
                </Border>
            </Grid>
        </ControlTemplate>

        <ControlTemplate x:Key="CutieTextBoxTemplate" TargetType="TextBox">
            <Border CornerRadius="17" Height="32" Background="White">
                <ScrollViewer Margin="10,7" x:Name="PART_ContentHost"/>
            </Border>
        </ControlTemplate>

        <ControlTemplate x:Key="CutieCombo" TargetType="ComboBox">
            <Grid Height="32" MaxHeight="32">
                <Border CornerRadius="17" Background="White">
                    <Grid>
                        <ToggleButton x:Name="DropDownToggle"/>
                        <ContentPresenter x:Name="ContentPresenter" />
                        <TextBlock Text=" " />
                    </Grid>
                </Border>
            </Grid>
        </ControlTemplate>

